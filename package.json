{
    "name": "time-tracker-cli",
    "version": "2.5.0",
    "description": "Tiny time tracker",
    "keywords": [
        "time",
        "tracker",
        "timer",
        "cli",
        "time log",
        "time logger",
        "logger"
    ],
    "bin": {
        "timer": "./bin/timer"
    },
    "main": "./lib/exports",
    "scripts": {
