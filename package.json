    "devDependencies": {
        "babel": "^6.3.26",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-stage-0": "^6.22.0",
        "prettier": "^1.10.2"
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "cli-table2": "^0.2.0",
        "colors": "^1.1.2",
        "commander": "^3.0.1",
        "configstore": "^3.1.1",
        "flat": "^4.0.0",
        "inquirer": "^3.3.0",
